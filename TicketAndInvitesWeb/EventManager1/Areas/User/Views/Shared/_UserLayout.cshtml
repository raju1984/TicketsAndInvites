                left: 0;
                right: 0;
                top: 0;
                bottom: 0;
                z-index: 100;
            }

            .dot {
                background: tomato;
            }

                .dot, .dot:after {
                    display: inline-block;
                    width: 2em;
                    height: 2em;
                    border-radius: 50%;
                    animation: a 1.5s calc(((var(--i) + var(--o, 0))/var(--n) - 1)*1.5s) infinite;
                }

                    .dot:after {
                        --o: 1;
                        background: currentcolor;
                        content: '';
                    }

            @@keyframes a {
                0%, 50% {
                    transform: scale(0);
                }
            }
