                                                    <thead>
                                                        <tr>
                                                            <th>Title</th>
                                                            <th>Name</th>
                                                            <th>Email</th>
                                                            <th>Mobile No</th>
                                                            <th>Seat no</th>
                                                            <th>Table no</th>
                                                            <th>Color Code</th>
                                                        </tr>
                                                    </thead>
